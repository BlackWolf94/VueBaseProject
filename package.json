    "ssr:dev": "ts-node --project tsconfig.webpack.json ssrDev/ssrDevServer.ts",
    "ssr:build": "rimraf dist && cross-env NODE_ENV=production ts-node --project tsconfig.webpack.json build/build.ts",
    "start": "npm run ssr:build && npm run server:start"
