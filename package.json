{
  "name": "nest-vue-ssr",
  "version": "0.0.0",
  "private": false,
  "description": "Vue SSR project base on typescript and use NestJs as server",
  "scripts": {
    "cli:i18n": "ts-node -P tsconfig.serve.json cli/i18n/index.ts",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "vue:lint": "vue-cli-service lint --fix",
    "vue:test": "vue-cli-service test",
    "dev": "tsnd --clear --respawn --debug -P ./tsconfig.serve.json src/server/main.ts",
    "start": "rimraf dist && cross-env NODE_ENV=production ts-node --project ./tsconfig.serve.json src/server/main.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix"
  },
  "dependencies": {
    "@mdi/font": "^4.5.95",
