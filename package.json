{
  "name": "vue-base",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "test": "vue-cli-service test",
    "ssr:dev": "ts-node --project tsconfig.ssr.json server.ts",
    "ssr:start": "cross-env NODE_ENV=production ts-node server",
    "ssr:build": "rimraf dist && npm run build:client && npm run build:server",
    "ssr:wp": "ts-node -p tsconfig.ssr.json ",
    "ssr:build:client": "cross-env NODE_ENV=production webpack --config build/webpack/client.conf.ts --progress --hide-modules",
    "ssr:build:server": "cross-env NODE_ENV=production webpack --config build/webpack/server.conf.ts --progress --hide-modules"
  },
  "dependencies": {
    "@mdi/font": "^4.5.95",
    "core-js": "^3.3.2",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.2",
    "@types/extract-text-webpack-plugin": "^3.0.4",
    "@types/friendly-errors-webpack-plugin": "^0.1.2",
    "@types/lru-cache": "^5.1.0",
    "@types/memory-fs": "^0.3.2",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack-node-externals": "^1.6.3",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "chokidar": "latest",
    "compression": "latest",
    "cross-env": "^6.0.3",
    "express": "latest",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "lint-staged": "^9.4.2",
    "lru-cache": "latest",
    "memory-fs": "latest",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "terser-webpack-plugin": "latest",
    "ts-node": "^8.4.1",
    "tsconfig-paths-webpack-plugin": "latest",
    "typescript": "~3.5.3",
    "vhost": "latest",
    "vhost-ts": "latest",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-cli-plugin-pug-template": "^0.20.0",
    "vue-cli-plugin-vuetify": "^1.1.1",
    "vue-loader": "latest",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0",
    "webpack": "latest",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "latest",
    "webpack-node-externals": "^1.7.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
